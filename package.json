    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.18.1",
    "i": "^0.3.7",
    "node-cron": "^3.0.1",
    "nodemon": "^1.19.4",
    "npm": "^8.19.3",
    "redis": "^4.2.0",
    "ts-node": "^10.9.1",

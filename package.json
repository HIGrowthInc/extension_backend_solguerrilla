{
  "name": "ino-backend",
  "version": "1.0.0",
  "description": "INO Api",
  "scripts": {
    "start": "ts-node server.ts",
    "start:prod": "npm run compile && node ./dist/server.js",
    "compile": "npm run transpile",
    "dev": "nodemon",
    "serve": "nodemon --delay 500ms server.js",
    "pm2": "NODE_ENV=production && npm run compile && pm2 start ./dist/server.js",
    "prod": "pm2 start server.ts --watch",
    "cron": "ts-node src/scripts/cron/index.ts",
    "seed": "ts-node src/scripts/seeder.ts",
    "sync_events": "ts-node src/scripts/past_events_sync.ts",
    "watch": "tsc -w",
    "transpile": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "keywords": [
    "Express",
    "MongoDB",
    "Babel",
    "Webpack Configuration"
  ],
  "author": "Jenson Collins",
  "license": "MIT",
  "devDependencies": {
